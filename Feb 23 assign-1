l=[]
n=int(input('enter n:'))
for i in range(n):
   c=int(input())
   l.append(c)
min=l[0]
for i in range(1,n):
   if l[i]<min:
     min=l[i]
max=l[0]
for i in range(1,n):
   if l[i]>max:
     max=l[i]
 
print("minimum",min) 
print("maximum",max)

output:
enter n:5
13
27
21
45
89
minimum 13
maximum 89
